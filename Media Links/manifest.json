{
   "background": {
      "service_worker": "background.js"
   },
   "commands": {
      "_execute_sidebar_action": {
         "description": "Open Media Links sidebar",
         "suggested_key": {
            "default": "Alt+M",
            "mac": "Alt+M"
         }
      }
   },
   "description": "Navigate IMDb and Letterboxd with a sidebar",
   "host_permissions": [ "https://www.imdb.com/", "https://letterboxd.com/", "https://www.cbfcindia.gov.in/" ],
   "key": "",
   "manifest_version": 3,
   "name": "Media Links",
   "permissions": [ "activeTab", "sidePanel", "scripting", "storage" ],
   "side_panel": {
      "default_path": "sidebar.html",
      "default_width": 300
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.62"
}
